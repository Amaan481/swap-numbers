#include <iostream>

using namespace std;

int amaan(int i)
{
    if(i<=10){
        cout<<"10 x"<<i<<"="<<10*i<<endl;
        i++;
        amaan(i);
    }
};
int naama(int i){
    if(i>=1){
        cout<<"10 x"<<i<<"="<<10*i<<endl;
        i--;
        naama(i);
    }
};
int main()
{
    int i=1;
    amaan(i);
    cout<<"------------AMAAN----------"<<endl;
    naama(i*10);
}
